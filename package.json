{
  "name": "minime",
  "version": "1.0.0",
  "description": "AI-powered stream alert system for Twitch streamers featuring lifelike avatars, real-time event notifications, and customizable alert scripts.",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "api:start": "cross-env IS_OFFLINE=true STAGE=local serverless offline start",
    "api:dev": "cross-env IS_OFFLINE=true STAGE=local serverless offline start --stage local --reloadHandler",
    "dev:full": "concurrently \"npm run dev\" \"npm run api:dev\"",
    "api:restart": "taskkill /F /IM node.exe /FI \"WINDOWTITLE eq serverless*\" 2>nul & npm run api:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chqrlotte/minime.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chqrlotte/minime/issues"
  },
  "homepage": "https://github.com/chqrlotte/minime#readme",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "patch-package": "^8.0.1",
    "serverless": "3.40.0",
    "serverless-esbuild": "^1.55.1",
    "serverless-offline": "^13.9.0",
    "serverless-prune-plugin": "^2.1.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.927.0",
    "@aws-sdk/client-ssm": "^3.927.0",
    "aws-sdk": "^2.1692.0",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jmespath": "^0.16.0",
    "stripe": "^19.1.0",
    "xml2js": "^0.6.2"
  }
}
